{
  "name": "@packages/lib",
  "type": "module",
  "private": true,
  "exports": {
    "./common/*": {
      "types": "./dist/common/*.d.ts",
      "import": {
        "development": "./src/common/*.ts",
        "default": "./dist/common/*.js"
      }
    }
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist && rm -f tsconfig.tsbuildinfo && rm -rf .turbo"
  },
  "devDependencies": {
    "@packages/eslint-config": "*",
    "@packages/ts-config": "*",
    "eslint": "^9.22.0",
    "typescript": "latest"
  }
}
